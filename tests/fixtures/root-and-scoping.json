{
  "commands": {
    "command-with-git-root": {
      "command": "echo 'WithRoot'",
      "root": {
        "path": "/command/root",
        "scope": "GitRoot"
      }
    },
    "global-group": {
      "commands": {
        "global-cmd": "echo 'Global'"
      },
      "root": {
        "path": "/nested/folder",
        "scope": "Global"
      }
    },
    "git-root-group": {
      "commands": {
        "git-root-cmd": "echo 'GitRoot'"
      },
      "root": {
        "path": "/nested/folder",
        "scope": "GitRoot"
      }
    },
    "exact-group": {
      "commands": {
        "exact-cmd": "echo 'Exact'"
      },
      "root": {
        "path": "/nested/folder",
        "scope": "Exact"
      }
    },
    "no-root-group": {
      "commands": {
        "no-root-cmd": "echo 'NoRoot'"
      }
    }
  },
  "root": {
    "path": "/root/folder"
  }
}
