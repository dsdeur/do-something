{
  "commands": {
    "command-no-env": {
      "command": "echo 'NoEnv'"
    },
    "group-env": {
      "commands": {
        "group-cmd": "echo 'GroupEnv'",
        "command-env": {
          "command": "echo 'CommandEnv'",
          "default_env": "command-vars",
          "envs": {
            "command-vars": {
              "vars": {
                "COMMAND_VAR": "command_value"
              }
            },
            "command-dotenv": ".command-env"
          }
        }
      },
      "default_env": "group-vars",
      "envs": {
        "group-config": {
          "path": ".group-env",
          "vars": {
            "GROUP_VAR": "group_value"
          }
        },
        "group-vars": {
          "vars": {
            "GROUP_VARS": "group_vars"
          }
        },
        "group-command": {
          "command": "echo 'GroupEnvCommand'"
        }
      }
    }
  },
  "envs": {
    "root-dotenv": ".root-env"
  }
}
