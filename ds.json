{
  "commands": {
    "install": {
      "aliases": ["i"],
      "command": "cargo build --release && cargo install --path ."
    },
    "run": {
      "aliases": ["r"],
      "command": "cargo run --"
    },
    "test": {
      "default_env": "dev",
      "commands": {
        "test": "echo $HELLO"
      },
      "envs": {
        "dev": {
          "vars": {
            "HELLO": "Hello, Development!"
          }
        },
        "prod": {
          "vars": {
            "HELLO": "Hello, Production!"
          }
        }
      }
    }
  }
}
