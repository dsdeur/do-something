{
  "commands": {
    "install": {
      "aliases": ["i"],
      "command": "cargo build --release && cargo install --path ."
    },
    "build": {
      "aliases": ["b"],
      "command": "cargo build --release"
    },
    "run": {
      "aliases": ["r"],
      "command": "cargo run --"
    },
    "clippy": {
      "aliases": ["cl"],
      "command": "cargo clippy --all-targets --all-features -- -D warnings"
    },
    "test": {
      "aliases": ["t"],
      "command": "cargo test"
    },
    "check": {
      "aliases": ["c"],
      "command": "ds clippy && ds test"
    }
  }
}
